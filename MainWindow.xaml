<Window x:Class="OpticalCharacterRecognition.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Open" Height="23" HorizontalAlignment="Right" Margin="0,0,225,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Read" Height="23" HorizontalAlignment="Left" Margin="203,48,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        
        <TextBox Height="23" HorizontalAlignment="Left" Name="textBox1" VerticalAlignment="Top" Width="206" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="0,48,0,0" Name="textBox2" VerticalAlignment="Top" Width="206" />
        <Image Height="234" HorizontalAlignment="Left" Margin="0,77,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="253" />
        <Image Height="234" HorizontalAlignment="Left" Margin="251,77,0,0" Name="image2" Stretch="Fill" VerticalAlignment="Top" Width="252" />
    </Grid>
</Window>
